{
	"name": "angular-minesweeper",
	"displayName": "Angular Minesweeper",
	"description": "Angular Minesweeper",
	"version": "0.0.1",
	"license": "mit",
	"private": true,
	"author": {
		"name": "Alex Aegis",
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis"
	},
	"homepage": "https://github.com/AlexAegis/angular-minesweeper",
	"bugs": {
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis/angular-minesweeper/issues"
	},
	"keywords": [
		"ng",
		"angular",
		"nx",
		"minesweeper",
		"rxjs",
		"reactive",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"sideEffects": [
		"./apps/*/src/main.ts"
	],
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "nx serve minesweeper",
		"build": "nx affected:build --all --prod",
		"build:minesweeper": "nx build minesweeper --prod --stats-json",
		"clean": "rimraf ./dist ./coverage ./.codacy-coverage ./cache",
		"self-destruct": "rimraf ./dist ./coverage ./.codacy-coverage ./cache ./node-modules ./package-lock.json ./yarn.lock ./pnpm-lock.yaml",
		"test": "nx run-many --all --target test",
		"test:skip-cache": "nx run-many --all --target test --skip-nx-cache",
		"lint": "nx workspace-lint && ng lint",
		"e2e": "nx e2e",
		"lint:ts": "tslint --project tsconfig.json",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:css": "stylelint --allow-empty-input --syntax=css ./**/*.css",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:format": "prettier --check .",
		"analyze:minesweeper": "webpack-bundle-analyzer dist/apps/minesweeper/stats.json",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build --prod",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu -u",
		"typedoc": "typedoc libs --tsconfig ./tsconfig.json --out ./dist/typedoc",
		"update": "ng update @nrwl/workspace",
		"update:all": "nx update --all --force",
		"workspace-schematic": "nx workspace-schematic",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --tsconfig tsconfig.json",
		"release": "standard-version",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"@angular/cdk": "^10.2.6",
		"@angular/common": "~10.2.0",
		"@angular/compiler": "~10.2.0",
		"@angular/core": "~10.2.0",
		"@angular/forms": "~10.2.0",
		"@angular/platform-browser": "~10.2.0",
		"@angular/platform-browser-dynamic": "~10.2.0",
		"@angular/router": "~10.2.0",
		"@nrwl/angular": "10.3.1",
		"core-js": "~3.6.5",
		"rxjs": "~6.6.3",
		"tslib": "^2.0.3",
		"zone.js": "~0.11.2"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^10.0.1",
		"@angular-devkit/build-angular": "~0.1002.0",
		"@angular/cli": "~10.2.0",
		"@angular/compiler-cli": "~10.2.0",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@nrwl/cypress": "~10.3.1",
		"@nrwl/eslint-plugin-nx": "~10.3.1",
		"@nrwl/jest": "~10.3.1",
		"@nrwl/linter": "~10.3.1",
		"@nrwl/node": "~10.3.1",
		"@nrwl/web": "~10.3.1",
		"@nrwl/workspace": "~10.3.1",
		"@types/jasmine": "~3.6.0",
		"@types/jasminewd2": "~2.0.8",
		"@types/node": "^14.14.5",
		"@typescript-eslint/eslint-plugin": "^4.6.0",
		"@typescript-eslint/parser": "^4.6.0",
		"codelyzer": "^6.0.1",
		"cypress": "~5.5.0",
		"compression-webpack-plugin": "^6.0.4",
		"eslint": "^7.12.1",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"jest": "~26.6.1",
		"jest-preset-angular": "8.3.2",
		"lint-staged": "^10.5.0",
		"markdownlint": "^0.21.1",
		"markdownlint-cli": "^0.24.0",
		"ng-packagr": "^10.1.2",
		"npm-check-updates": "^9.2.3",
		"nyc": "^15.1.0",
		"prettier": "^2.1.2",
		"rimraf": "^3.0.2",
		"rollup": "~2.32.1",
		"rollup-plugin-gzip": "~2.5.0",
		"rollup-plugin-terser": "~7.0.2",
		"standard-version": "~9.0.0",
		"stylelint": "^13.7.2",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-prettier": "^1.1.2",
		"ts-jest": "~26.4.3",
		"ts-node": "~9.0.0",
		"tslint": "~6.1.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.3.0",
		"typedoc": "~0.19.2",
		"typedoc-plugin-external-module-name": "~4.0.3",
		"typescript": "~4.0.3",
		"webpack-bundle-analyzer": "^3.9.0",
		"yaml-lint": "^1.2.4",
		"@types/jest": "26.0.15"
	}
}
